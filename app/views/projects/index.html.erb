<%- model_class = Project -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>

<% @projects.each do |project| %>
  <%= render project %>
<% end %>


<%= link_to "New Project", new_project_path, :class => 'btn btn-primary' if user_signed_in? %>
